@model IEnumerable<Secretaria.Domain.DatosPersonales.Localidad>
<div class="row">
    <div class="col-10">
        <h1 class="text-monospace">Localidad</h1>
    </div>
</div>
<br />
<table class="table table-bordered table-hover table-sm">
        <thead class="thead-dark">
            <tr>
                <th>id</th>
                <th>Localidad</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var localidad in Model)
            {
            <tr>
                <td>@localidad.Id</td>
                <td>@localidad.Cadena</td>
            </tr>
            }
        </tbody>
</table>
<div class="form-group col-6">
    <a role="button" asp-controller="Localidad" asp-action="AgregarLocalidad" class="btn btn-outline-primary">Agregar Localidad</a>
    <a asp-controller="Home" asp-action="Index" role="button" class="btn btn-outline-secondary">Volver</a>
</div>
